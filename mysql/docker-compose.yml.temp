mysql:
  image: --MYSQL_IMAGE_URI--:--MYSQL_IMAGE_VERSION--
  net: host
  environment:
    - CONFIG_SERVER=http://--CONFIGSERVER_IP--:--CONFIGSERVER_PORT--
    - SERVICE=zabbix/zabbix-mysql
    - NOHOSTNAME=true
    - MYSQL_ROOT_PASSWORD=--ZBX_MYSQL_ROOT_PASS--
    - MYSQL_DATABASE=--ZBX_MYSQL_DATABASE--
#  env_file:
#    - ./mysql.env
  volumes:
    - /data/volumn/mysql:/var/lib/mysql:rw
